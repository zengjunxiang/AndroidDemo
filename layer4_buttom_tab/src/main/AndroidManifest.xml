<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zengjunxiang.androiddemo.layer4_buttom_tab">

    <application android:allowBackup="true" android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name" android:supportsRtl="true" >
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".activity.RadioGroupTabActivity"/>
        <activity android:name=".activity.FragmentTabHostActivity"
            android:theme="@style/AppTheme"/>

        <activity android:name=".activity.TabLayoutActivity"
            android:theme="@style/AppTheme"/>

        <activity android:name=".activity.BottomNavigationViewActivity"
            android:theme="@style/AppTheme"/>

        <activity android:name=".activity.CustomTabViewActivity"
            android:theme="@style/AppTheme" />



    </application>



</manifest>